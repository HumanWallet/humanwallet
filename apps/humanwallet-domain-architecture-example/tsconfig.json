{
  "compilerOptions": {
    "lib": ["es2023", "dom"],
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "types": ["node", "vite/client"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler"
  }
}
